  "version": "0.2.0",
  "configurations": [
    {
      "name": "controls-gdbserver",
      "type": "cppdbg",
      "request": "launch",
      "program": "/root/dev/robosub-ros2/onboard/install/controls/lib/controls/controls",
      "miDebuggerServerAddress": "localhost:3000",
      "cwd": "/root/dev/robosub-ros2/onboard/src/controls",
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        },
        {
          "description": "Set Disassembly Flavor to Intel",
          "text": "-gdb-set disassembly-flavor intel",
          "ignoreFailures": true
        }
      ]
    },
    {
      "name": "controls-gdb",
      "type": "cppdbg",
      "request": "launch",
      "program": "/root/dev/robosub-ros2/onboard/install/controls/lib/controls/controls",
      "args": [],
      "stopAtEntry": false,
      "cwd": "/root/dev/robosub-ros2/onboard/src/controls",
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        },
        {
          "description": "Set Disassembly Flavor to Intel",
          "text": "-gdb-set disassembly-flavor intel",
          "ignoreFailures": true
        }
      ]
    },
  ]
}