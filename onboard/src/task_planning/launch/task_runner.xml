<launch>
	<arg name="bypass" doc='{"type":"bool","allowEmpty":true,"help":"Whether to bypass wait_for_service()"}'/>
	<arg name="untethered" doc='{"type":"bool","allowEmpty":true,"help":"Whether to run untethered"}'/>
	<node pkg="task_planning" name="task_runner" type="task_runner.py" output="screen">
		<param name="bypass" value="$(arg bypass)" />
		<param name="untethered" value="$(arg untethered)" />
	</node>
</launch>