<?xml version="1.0" encoding="UTF-8"?>
<launch>
	<arg name="stream" default="true" />
	<arg name="debug" default="false" />
	<node pkg="sonar" name="sonar" exec="sonar" output="screen">
		<param name="stream" value="$(var stream)" />
		<param name="debug" value="$(var debug)" />
	</node>
</launch>
